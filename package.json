    "files": [
        "./src/convertIFCDataToXKT.js",
        "./src/convertIFCFileToXKT.js",
        "./src/convertIFCIntoXKTModel.js"
    ],
    "dependencies": {
        "@xeokit/xeokit-xkt-utils": "1.1.2",
        "commander": "^2.20.0",
        "web-ifc": "0.0.17"
