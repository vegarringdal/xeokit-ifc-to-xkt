    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xeokit/xeokit-ifc-to-xkt.git"
    },
    "keywords": [
        "xeokit",
        "bim",
        "ifc",
        "xkt",
        "webgl",
        "3d",
        "wasm"
    ],
